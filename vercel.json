{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "src/server/index.ts",
      "use": "@vercel/node",
      "config": {
        "skipTypeCheck": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server/index.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 